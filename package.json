{
  "name": "mdpages",
  "description": "Quickly create and deploy static markdown pages with Next.js",
  "keywords": [
    "markdown",
    "static",
    "pages",
    "static-pages",
    "next",
    "Next.js"
  ],
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "yarn favicon && next dev",
    "build": "yarn favicon && next build",
    "start": "next start",
    "lint": "next lint",
    "cname": "node bin/generateCNAME.js",
    "favicon": "node --loader ts-node/esm bin/generateFavicon.mts"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "fs-extra": "^11.2.0",
    "github-markdown-css": "^5.5.0",
    "gray-matter": "^4.0.3",
    "mdast-util-to-markdown": "^2.1.0",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "sass": "^1.70.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "favicons": "^7.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "packageManager": "yarn@4.3.1"
}
